{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*", "./src/**/* "],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "jsxImportSource": "preact",
    // "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"],
      "/src/*": ["./src/*"],
      "@comp/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@elements/*": ["./src/elements/*"]
    }
  }
}
